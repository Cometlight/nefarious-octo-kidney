<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import org.controlsfx.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="_root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="250.0" minWidth="750.0" spacing="14.0" stylesheets="@../../css/simple.css" HBox.hgrow="ALWAYS">
         <children>
            <HBox spacing="14.0" styleClass="simple-heading" VBox.vgrow="ALWAYS">
               <children>
                  <Label text="Choose team members" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="14.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0" spacing="14.0" HBox.hgrow="ALWAYS">
                     <children>
                        <HBox spacing="14.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Label styleClass="simple-label" text="Tournament: " HBox.hgrow="ALWAYS" />
                              <Label fx:id="_tournamentLabel" styleClass="simple-label" text="_tournament" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox prefHeight="100.0" prefWidth="200.0" spacing="14.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Label styleClass="simple-label" text="Team: " HBox.hgrow="ALWAYS" />
                              <Label fx:id="_teamLabel" styleClass="simple-label" text="_team" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <CheckListView fx:id="_teamMemberCheckListView" HBox.hgrow="ALWAYS" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
            <HBox spacing="14.0" VBox.vgrow="ALWAYS">
               <children>
                  <Pane HBox.hgrow="ALWAYS" />
                  <Button mnemonicParsing="false" onAction="#_cancelButtonOnActionEventHandler" styleClass="simple-red" text="Cancel" />
                  <Button mnemonicParsing="false" onAction="#_acceptButtonOnActionEventHandler" styleClass="simple-green" text="Accept" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </padding>
      </VBox>
   </children>
</HBox>
