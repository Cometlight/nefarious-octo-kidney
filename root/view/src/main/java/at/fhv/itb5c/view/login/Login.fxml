<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="37.0" minHeight="1.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="59.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints fillHeight="false" maxHeight="59.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Loginname" />
            <Label text="Passwore" GridPane.rowIndex="1" />
            <Button fx:id="_loginButton" mnemonicParsing="false" onMouseReleased="#LoginButton_OnMouseReleased_Button_Handler" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField GridPane.columnIndex="1" />
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Separator prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </center>
   <top>
      <Pane prefHeight="2.0" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <Pane prefHeight="2.0" BorderPane.alignment="CENTER" />
   </bottom>
   <right>
      <Pane prefWidth="2.0" BorderPane.alignment="CENTER" />
   </right>
   <left>
      <Pane prefWidth="2.0" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
