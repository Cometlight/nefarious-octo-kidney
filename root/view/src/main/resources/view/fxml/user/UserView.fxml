<?xml version="1.0" encoding="UTF-8"?>

<?import org.controlsfx.control.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>

<BorderPane fx:id="_borderPane" stylesheets="@../../css/BaseStile.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Label fx:id="_titelLabel" styleClass="label-header" text="Create New User" BorderPane.alignment="TOP_LEFT" />
   </top>
   <center>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="NEVER" />
            <ColumnConstraints hgrow="NEVER" prefWidth="50.0" />
          <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints minWidth="10.0" />
            <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="FirstName" />
            <Label text="LastName" GridPane.rowIndex="1" />
            <Label text="Adress" GridPane.rowIndex="2" />
            <Label text="Telephone Number" GridPane.rowIndex="3" />
            <Label text="eMail" GridPane.rowIndex="4" />
            <Label text="Gender" GridPane.rowIndex="5" />
            <Label text="Date of Birth" GridPane.rowIndex="6" />
            <Label text="Type of Sport" GridPane.rowIndex="7" />
            <Label text="Membership Fee" GridPane.rowIndex="8" />
            <Label text="Role" GridPane.rowIndex="9" />
            <DatePicker fx:id="_birthdayDatePicker" GridPane.columnIndex="3" GridPane.rowIndex="6" />
            <TextField fx:id="_memebershipFeeTextBox" GridPane.columnIndex="3" GridPane.rowIndex="8" />
            <ComboBox fx:id="_genderComboBox" onAction="#genderComboBoxOnActionEventHandler" GridPane.columnIndex="3" GridPane.rowIndex="5" />
            <TextField fx:id="_eMailTextField" GridPane.columnIndex="3" GridPane.rowIndex="4" />
            <TextField fx:id="_telephoneNumberTextField" GridPane.columnIndex="3" GridPane.rowIndex="3" />
            <TextField fx:id="_adressTextField" GridPane.columnIndex="3" GridPane.rowIndex="2" />
            <TextField fx:id="_lastNameTextField" GridPane.columnIndex="3" GridPane.rowIndex="1" />
            <TextField fx:id="_firstNameTextField" GridPane.columnIndex="3" />
            <CheckListView fx:id="_typeOfSportCheckListView" prefHeight="75.0" GridPane.columnIndex="3" GridPane.rowIndex="7" />
            <Pane fx:id="_controlPane" GridPane.columnIndex="5" GridPane.rowIndex="9" />
            <CheckListView fx:id="_userRoleCheckListView" maxHeight="75.0" GridPane.columnIndex="3" GridPane.rowIndex="9" />
         </children>
      </GridPane>
   </center>
</BorderPane>
