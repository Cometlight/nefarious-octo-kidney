<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<Pane minWidth="-Infinity" prefHeight="473.0" prefWidth="988.0"
	stylesheets="@../css/BaseStile.css" 
	xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<GridPane prefHeight="473.0" prefWidth="988.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" />
				<ColumnConstraints hgrow="SOMETIMES" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="285.0"
					minWidth="275.0" prefWidth="275.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints vgrow="SOMETIMES" />
				<RowConstraints vgrow="SOMETIMES" />
				<RowConstraints vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<TableView GridPane.rowIndex="2">
					<columns>
						<TableColumn maxWidth="129.0" minWidth="100.0"
							prefWidth="113.0" text="First Name" />
						<TableColumn maxWidth="116.0" minWidth="71.0"
							prefWidth="110.0" text="Last Name" />
						<TableColumn maxWidth="108.0" minWidth="71.0"
							prefWidth="92.0" text="Address" />
						<TableColumn prefWidth="144.0" text="Date of Birth" />
						<TableColumn maxWidth="186.0" minWidth="28.0"
							prefWidth="186.0" text="Membership Fee" />
					</columns>
				</TableView>
				<GridPane GridPane.columnIndex="2" GridPane.rowIndex="2">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints maxHeight="208.0" minHeight="10.0"
							prefHeight="150.0" vgrow="SOMETIMES" />
						<RowConstraints vgrow="SOMETIMES" />
						<RowConstraints maxHeight="-Infinity" minHeight="-Infinity"
							prefHeight="25.0" vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<GridPane>
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES" minWidth="-Infinity" />
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
								<ColumnConstraints hgrow="SOMETIMES" />
							</columnConstraints>
							<rowConstraints>
								<RowConstraints vgrow="SOMETIMES" />
								<RowConstraints vgrow="SOMETIMES" />
								<RowConstraints vgrow="SOMETIMES" />
								<RowConstraints minHeight="10.0" prefHeight="30.0"
									vgrow="SOMETIMES" />
							</rowConstraints>
							<children>
								<Label text="FirstName" />
								<Label text="LastName" GridPane.rowIndex="1" />
								<Label text="Date of Birth" GridPane.rowIndex="2" />
								<Label text="Membership Fee" GridPane.rowIndex="3" />
								<TextField fx:id="_firstNameTextField"
									GridPane.columnIndex="2" />
								<TextField fx:id="_lastNameTextField"
									GridPane.columnIndex="2" GridPane.rowIndex="1" />
								<CheckBox fx:id="_paidCheckBox" mnemonicParsing="false"
									text="Paid" GridPane.columnIndex="2" GridPane.rowIndex="3" />
								<DatePicker fx:id="_dateOfBirthDatePicker"
									GridPane.columnIndex="2" GridPane.rowIndex="2" />
							</children>
							<padding>
								<Insets left="10.0" right="10.0" />
							</padding>
						</GridPane>
						<GridPane GridPane.rowIndex="2">
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES" />
								<ColumnConstraints hgrow="SOMETIMES" />
							</columnConstraints>
							<rowConstraints>
								<RowConstraints vgrow="SOMETIMES" />
							</rowConstraints>
							<children>
								<Button mnemonicParsing="false"
									text="Cancel" onMouseReleased="#cancelButtonOnMouseReleasedEventHandler"/>
								<Button mnemonicParsing="false"
									text="Search" GridPane.columnIndex="1" 
									onMouseReleased="#searchButtonOnReleasedEventHandler"/>
							</children>
						</GridPane>
					</children>
				</GridPane>
				<Separator orientation="VERTICAL" prefHeight="200.0"
					GridPane.columnIndex="1" GridPane.rowIndex="2" />
				<Label styleClass="label-header" stylesheets="@../css/BaseStile.css"
					text="Search User" />
				<Separator minHeight="5.0" GridPane.columnSpan="3"
					GridPane.rowIndex="1" />
			</children>
		</GridPane>
	</children>
</Pane>

